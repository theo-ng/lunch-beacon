  },
  "dependencies": {
    "@material-ui/core": "^3.0.3",
    "@material-ui/icons": "^3.0.1",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "express": "^4.16.2",
    "firebase-admin": "^6.0.0"
  },
  "devDependencies": {
    "react-scripts": "1.1.5",
    "concurrently": "^3.5.0"
  }
}
